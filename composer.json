{
    "name": "theme-anorak/laravel-shopify",
    "description": "Api Wrapper for using Shopify in a Laravel application",
    "type": "library",
    "license": "MIT",
    "require": {
        "dpc/hash-verifier": "dev-master",
        "dpc/guzzle-client": "dev-master"
    },
    "require-dev": {
    },
    "autoload": {
